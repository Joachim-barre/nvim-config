{
    "disabled_modules" : [],
    "sidebar" : {
        "enabled" : 0,
        "sections" : [
            "datetime", "actions", "files", "git" ,"diagnostics"
        ]
    },
    "local-scripts" : [],
    "global-scripts" : [],
    "run" : {
        "java" : [
            "cd $dir &&",
            "javac $fileName &&",
            "java $fileNameWithoutExt"
        ],
        "python" : "python3 $file $end",
        "sh" : "zsh",
        "c" : [
            "cd $dir &&",
            "clang $fileName",
            "-o $fileNameWithoutExt &&",
            "$dir/$fileNameWithoutExt"
        ],
        "c++" : [
            "cd $dir &&",
            "clang $fileName",
            "-o $fileNameWithoutExt &&",
            "$dir/$fileNameWithoutExt"
        ],
        "rust" : "cargo test && cargo run $end"
    },
    "lspconfig" : {
        "cmp": true,
        "rust": true,
        "pyright": true,
        "clangd": true,
        "csharp": true
    }
}
